	    public static int SurfaceNormals = 6;
	}

	class Drivetraintype {
		public static int MaxDegreeOfFreedom = 0;
		public static int ForwardOnly = 1;    
	}

	class LandedState {
		public static int Landed = 0;
		public static int Flying = 1;
	}
}
